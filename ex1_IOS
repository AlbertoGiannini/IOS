//EX 1
var inteiroA: Int = 1
var inteiroB = 2
var stringA: String = "textoA"
var stringB = "textoB"
var doubleA: Double = 1.0
var doubleB = 2.0
var boolA: Bool = false
var boolB = true
print(inteiroA, inteiroB, stringA, stringB, doubleA, doubleB, boolA, boolB)

